{
  "name": "cordova-react-onsenui-starter",
  "version": "1.0.0",
  "description": "starter kit for cordova with react and onsenui",
  "scripts": {
    "clean": "rimraf www\\bundle.js.map && rimraf www\\*.ttf && rimraf www\\*.eot && rimraf www\\*.woff && rimraf www\\*.woff2 && rimraf www\\*.svg && rimraf www\\*.PNG && rimraf www\\*.png",
    "android_prod": "npm run clean & webpack --config webpack-pro.config.js --progress --colors -p && cordova run android",
    "android_dev": "webpack --config webpack-dev.config.js --progress --colors && cordova run android",
    "android_dev_2": "webpack --config webpack-dev.config.js --progress --colors && cordova prepare android && cordova build android && cordova run android",
    "win_dev": "webpack --config webpack-dev.config.js --progress --colors && cordova run windows",
    "ios_dev": "webpack --config webpack-dev.config.js --progress --colors && cordova run ios",
    "ios_prod": "npm run clean & webpack --config webpack-pro.config.js --progress --colors -p && cordova run ios",
    "build_dev": "webpack --config webpack-dev.config.js --progress --colors",
    "web_dev": "webpack-dev-server --config webpack-dev.config.js --progress && cordova run windows",
    "web_pro": "npm run clean & webpack --config webpack-pro.config.js --progress --colors -p"
    },
  "author": "Yoav Nino",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-literals": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "history": "^4.3.0",
    "less-loader": "^2.2.3",
    "style-loader": "^0.13.1",
    "react-hot-loader": "^3.0.0-beta.5",
    "react-onsenui": "^1.1.0",
    "react-router": "^2.8.1",
    "redux-react-router": "^1.0.0-beta3",
    "transfer-webpack-plugin": "*",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.22",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "2.1.0-beta.10",
    "webpack-hot-middleware": "^2.2.0",
    "webpack-version-file": "^0.1.2"
  },
  "dependencies": {
    "onsenui": "2.2.5",
    "less": "^2.7.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1"
    }
}
